{
  "name": "@teddy-monorepo/api/feature-health",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/feature-health/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "tsc --build tsconfig.lib.json",
        "cwd": "libs/api/feature-health"
      },
      "cache": true,
      "inputs": [
        "{projectRoot}/package.json",
        "{workspaceRoot}/tsconfig.base.json",
        "{projectRoot}/tsconfig.lib.json",
        "{projectRoot}/src/**/*.ts",
        "^production"
      ],
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --build --emitDeclarationOnly tsconfig.lib.json",
        "cwd": "libs/api/feature-health"
      },
      "cache": true,
      "inputs": [
        "{projectRoot}/package.json",
        "{workspaceRoot}/tsconfig.base.json",
        "{projectRoot}/tsconfig.lib.json",
        "{projectRoot}/src/**/*.ts",
        "!{projectRoot}/src/**/*.spec.ts",
        "^production"
      ]
    }
  }
}
